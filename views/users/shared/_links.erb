<%- if controller_name != 'sessions' %>
  <div class="inputwrap_login passforgot clearfix" style="padding-bottom: 0;">	
    <label>&nbsp;</label>
    <%= link_to "Sign in", new_session_path(resource_name) %>
  </div>
<% end -%>

<!--
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Sign up", new_registration_path(resource_name) %><br />
<% end -%>
-->

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <div class="inputwrap_login passforgot clearfix" style="padding-bottom: 0;">		
    <label>&nbsp;</label>
    <%= link_to "forgot password?", new_password_path(resource_name), :class => 'forgot' %>
  </div>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <div class="inputwrap_login passforgot clearfix" style="padding-bottom: 0;">	
    <label>&nbsp;</label>
    <%= link_to "Didn't receive confirmation instructions?", new_user_confirmation_path %>
  </div>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <dl>			
    <dt>&nbsp;</dt>
    <dd><%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) %></dd>
  </dl>
<% end -%>

